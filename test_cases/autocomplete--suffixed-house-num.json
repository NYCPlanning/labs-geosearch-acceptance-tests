{
  "name": "autocomplete - suffixed house number",
  "endpoint": "autocomplete",
  "priorityThresh": 5,
  "tests": [
    {
      "id": "1",
      "status": "pass",
      "in": {
        "text": "120 1/2 first ave"
      },
      "expected": {
        "properties": [
          {
            "name": "120 1/2 FIRST AVENUE",
            "label": "120 1/2 FIRST AVENUE, Manhattan, New York, NY, USA"
          }
        ]
      }
    },
    {
      "id": "2",
      "status": "pass",
      "in": {
        "text": "240-55 1/3 depew ave"
      },
      "expected": {
        "properties": [
          {
            "name": "240-55 1/3 DEPEW AVENUE"
          }
        ]
      }
    },
    {
      "id": "3",
      "status": "pass",
      "in": {
        "text": "519 front east 12 st"
      },
      "expected": {
        "properties": [
          {
            "name": "519 FRONT EAST 12 STREET"
          }
        ]
      }
    }
  ]
}
